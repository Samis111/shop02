<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.it.mapper.ConfigMapper">

    <resultMap id="BaseResultMap" type="com.it.domain.Config">
            <id property="id" column="id" jdbcType="INTEGER"/>
            <result property="key" column="key" jdbcType="VARCHAR"/>
            <result property="valueurl" column="valueurl" jdbcType="VARCHAR"/>
            <result property="infotime" column="infotime" jdbcType="TIMESTAMP"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,key,valueurl,
        infotime
    </sql>
</mapper>
